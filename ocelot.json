      "DownstreamHostAndPorts": [
        {
          "Host": "get-links.openfaas-fn.svc",
          "Port": 8080
        }
      ],
      "UpstreamPathTemplate": "/get-links",
      "UpstreamHttpMethod": [
        "Get"
      ],
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "TestKey",
        "AllowedScopes": [
          "timon"
        ]
      }
    },
    {
      "DownstreamPathTemplate": "/login",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "timon-identity-server.openfaas-fn.svc",
          "Port": 8433
        }
      ],
      "UpstreamPathTemplate": "/login",
      "UpstreamHttpMethod": [
        "Post"
      ]
    },
    {
      "DownstreamPathTemplate": "/register",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "timon-identity-server.openfaas-fn.svc",
          "Port": 8433
        }
      ],
      "UpstreamPathTemplate": "/register",
      "UpstreamHttpMethod": [
        "Post"
      ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://api-gateway-myproject.127.0.0.1.nip.io/"
  }
